<?xml version="1.0" encoding="UTF-8"?>
<!--信用卡对账文件格式 -->
<!-- 
templateType=READ|WRITE 表示解析文件还是生成文件 
split分隔符 "\|" , 读文件时竖线分隔需要带转换符，写文件时不需要
headLength文件头的长度，带换行符的表示文件头占了多少行，不带换行符的应该是字节长度
endingLength文件尾
lineSeparator换行符，若无，则表示不换行，换行一般是\r\n 字节文件一般是固定长度不带分隔符不换行
-->

<data templateType="READ"
    split="\|" 
    clazz="com.scrcu.ebank.ebap.batch.bean.dto.DebitTranInfo" 
    charSet="GBK"
    headLength=""
    endingLength=""
    lineSeparator="rn">

     <column name="channelNo" length="" regex="" dateFormat="" numMultiple="" desc="渠道号" tranSrc="" tranDest="" /> 
     <column name="channelDate" length="" regex="" dateFormat="" numMultiple="" desc="渠道日期" tranSrc="" tranDest="" /> 
     <column name="channelSeq" length="" regex="" dateFormat="" numMultiple="" desc="渠道流水" tranSrc="" tranDest="" /> 
     <column name="txnDate" length="" regex="" dateFormat="" numMultiple="" desc="交易日期" tranSrc="" tranDest="" /> 
     <column name="platformSeq" length="" regex="" dateFormat="" numMultiple="" desc="交易流水" tranSrc="" tranDest="" /> 
     
     <column name="txnStatus" length="" regex="" dateFormat="" numMultiple="" desc="交易状态" tranSrc="" tranDest="" /> 
     <column name="txnCode" length="" regex="" dateFormat="" numMultiple="" desc="交易号" tranSrc="" tranDest="" /> 
     <column name="txnOrg" length="" regex="" dateFormat="" numMultiple="" desc="交易机构" tranSrc="" tranDest="" /> 
     <column name="txnTeller" length="" regex="" dateFormat="" numMultiple="" desc="交易柜员" tranSrc="" tranDest="" />
     <column name="txnCur" length="" regex="" dateFormat="" numMultiple="" desc="交易币种" tranSrc="" tranDest="" />
     
     <column name="payAccount" length="" regex="" dateFormat="" numMultiple="" desc="付款账户" tranSrc="" tranDest="" />
     <column name="receiveAccount" length="" regex="" dateFormat="" numMultiple="" desc="收款账户" tranSrc="" tranDest="" />
     <column name="txnAmount" length="" regex="" dateFormat="" numMultiple="" desc="交易金额" tranSrc="" tranDest="" />
     <column name="reserved1" length="" regex="" dateFormat="" numMultiple="" desc="格式问题处理" tranSrc="" tranDest="" />
</data>