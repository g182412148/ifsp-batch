<?xml version="1.0" encoding="UTF-8"?>
<!-- 贷款系统放款确认文件 -->
<!-- 
templateType=READ|WRITE 表示解析文件还是生成文件 
split分隔符 "\|" , 读文件时竖线分隔需要带转换符，写文件时不需要
headLength文件头的长度，带换行符的表示文件头占了多少行，不带换行符的应该是字节长度
endingLength文件尾
lineSeparator换行符，若无，则表示不换行，换行一般是\r\n 字节文件一般是固定长度不带分隔符不换行
-->
<data templateType="WRITE"
    split="|#|" 
    clazz="com.scrcu.ebank.ebap.batch.bean.dto.PaySubOrderInfo" 
    charSet="GBK"
    headLength=""
    endingLength=""
    lineSeparator="rn"
    version="3"
    >
<!-- 
regex正则表达式 
dateFormat日期格式
numMultiple数字类型需要转换到倍数
tranSrc要转换到源值，逗号分隔
tranDest要转换到目标值，逗号分隔
-->	
	<column name="openClientId" length="" regex="" dateFormat="" numMultiple="" desc="ClientID" tranSrc="" tranDest="" />
	<column name="customerId" length="" regex="" dateFormat="" numMultiple="" desc="客户号" tranSrc="" tranDest="" />
	<column name="subMchtId" length="" regex="" dateFormat="" numMultiple="" desc="收单商户号" tranSrc="" tranDest="" />
	<column name="payAmount" length="" regex="" dateFormat="" numMultiple="" desc="支付总金额" tranSrc="" tranDest="" />
	<column name="transFactTime" length="" regex="" dateFormat="" numMultiple="" desc="交易日期" tranSrc="" tranDest="" />
	<column name="detailsExtend1" length="" regex="" dateFormat="" numMultiple="" desc="预留字段1" tranSrc="" tranDest="" />
	<column name="detailsExtend1" length="" regex="" dateFormat="" numMultiple="" desc="预留字段2" tranSrc="" tranDest="" />
	<column name="detailsExtend1" length="" regex="" dateFormat="" numMultiple="" desc="预留字段3" tranSrc="" tranDest="" />
</data>