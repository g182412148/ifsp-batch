	<select id="selectAllInfo" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from MCHT_BASE_INFO
		where mcht_state not in ('10','12','99') and to_char(REG_DATE,'yyyymmdd') = #{regDate,jdbcType=VARCHAR}
		union
		select
		<include refid="Base_Column_List" />
		from MCHT_BASE_INFO
		where mcht_state not in ('10','12','99') and mcht_id in (select mcht_id from pmp_audit_info where audit_type='01' and audit_state='01'
		and substr(last_upd_date_time,1,8) = #{regDate,jdbcType=VARCHAR}
		)
	</select>

