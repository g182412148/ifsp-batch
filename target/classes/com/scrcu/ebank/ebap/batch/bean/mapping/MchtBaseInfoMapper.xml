	<select id="querySubbranchByMchtId" resultMap="BaseResultMap">
		select <include refid="Base_Column_List" />
		from MCHT_BASE_INFO WHERE PAR_MCH_ID =  #{mchtId ,jdbcType=VARCHAR} ORDER BY MCHT_ID
	</select>
	
	<select id="queryById" resultMap="BaseResultMap">
		select * from MCHT_BASE_INFO where mcht_id=#{mchtId}
	</select>

	<!-- 根据商户号查商户信息 -->
    <select id="selectMerInfoByMchtId" parameterType="java.util.Map" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from MCHT_BASE_INFO
        where MCHT_ID = #{mchtId}
    </select>

	<!-- 根据商户号集合批量查商户信息 -->
	<select id="selectMerInfoByMchtIdList" parameterType="java.util.Map" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from MCHT_BASE_INFO
		where MCHT_ID in
		<foreach collection="mchtIdList" index="index" item="item" open="(" separator="," close=")">
			#{item}
		</foreach>
	</select>

<!--     	List<MchtBaseInfoVo> queryMchtTypeByMchtId(String mchtId); -->
	<select id="queryMchtTypeByMchtId" parameterType="string" resultMap="MchtBaseInfoVo">
		    select *
      from MCHT_BASE_INFO mbi
     inner join MCHT_TYPE_RULE_INFO mtri on mbi.mcht_type = mtri.type_no
     where mbi.mcht_id = #{mchtId}
       and mtri.rule_no in('0003','0005')
	</select>

	<select id="queryMchtExtInfoByMchtId" parameterType="string" resultMap="MchtExtInfoMap">
	select a.*,c.PAGY_CT_CODE
	  from mcht_base_info a
	  left outer join pagy_ct_info c on (a.area_no = c.pagy_canton_code)
	 where a.mcht_id = #{mchtId}
	</select>

	<select id="selectUntradedMerchants" parameterType="string" resultMap="BaseResultMap">
		SELECT *
		FROM MCHT_BASE_INFO A
		WHERE A.MCHT_STATE = '01'
		<![CDATA[AND A.CRT_TM <= ADD_MONTHS(TO_DATE(#{orderTM ,jdbcType=VARCHAR},'YYYYMMDD'), #{months ,jdbcType=VARCHAR})]]>
		<![CDATA[ AND NVL(A.ORDER_TM_MAX,'0') <= TO_CHAR(ADD_MONTHS(TO_DATE(#{orderTM ,jdbcType=VARCHAR},'YYYYMMDD'), #{months ,jdbcType=VARCHAR}),'YYYYMMDD')]]>
	</select>





